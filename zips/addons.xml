<addons>
<addon id="plugin.program.bangwiz123" name="Bang Wizard" version="4.6" provider-name="Bang">
  <requires>
    <import addon="xbmc.python" version="3.0.0"/>
    <import addon="script.module.six" />
    <import addon="script.module.kodi-six" />
    <import addon="script.kodi.android.update" optional="true" version="1.3.2" />
  </requires>
  <extension point="xbmc.python.pluginsource" library="default.py">
        <provides>executable</provides>
  </extension>
  <extension point="xbmc.service" library="startup.py" start="startup" />
  <extension point="xbmc.addon.metadata">
    <summary lang="en">Bang Wizard</summary>
    <description lang="en">Bang Wizard - All Builds in one place</description>
    <platform>all</platform>
    <assets>
        <icon>icon.png</icon>
        <fanart>fanart.jpg</fanart>
        <banner></banner>
        <clearlogo></clearlogo>
        <screenshot></screenshot>
    </assets>
  </extension>
</addon>

